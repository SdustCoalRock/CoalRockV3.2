        +'0,DZ_Zhqk,支护方法,list,1,List_Zhqk;'    //   List_Zhqk:='单体支护,综采支架,放顶煤支架';
        +'0,S_Jc_L,累计进尺,edit,1,;'    //  11 工作面累计进尺
        +'0,S_Mtyd,煤体硬度,list,1,list_mtyd;'    // 12  煤体的普氏系数
        +'0,S_M_mcj,【煤】内摩擦角,edit,1,;'    //  13  煤体的内摩擦角
